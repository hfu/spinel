{
  "version": 8,
  "name": "hands-on example",
  "metadata": {},
  "sources": {
    "hands-on": {
      "type": "vector",
      "minzoom": 0,
      "maxzoom": 15,
      "attribution": "OpenStreetMap (c) OpenStreetMap contributors / Global Map Sri Lanka (c) ISCGM / Survey Department, Sri Lanka",
      "tiles": ["http://vectortiles.xyz:8765/zxy/hands-on/{z}/{x}/{y}.pbf"]
    }
  },
  "glyphs": "http://vectortiles.xyz:8765/fonts/{fontstack}/{range}.pbf",
  "sprite": "http://vectortiles.xyz:8765/sprite",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-color": "rgba(238, 238, 238, 1)"}
    },
    {
      "id": "Coastline",
      "type": "line",
      "source": "hands-on",
      "source-layer": "Coastline",
      "minzoom": 0,
      "maxzoom": 6,
      "paint": {"line-color": "rgba(21, 101, 192, 1)"}
    },
    {
      "id": "coastl",
      "type": "line",
      "source": "hands-on",
      "source-layer": "coastl",
      "minzoom": 6,
      "maxzoom": 22,
      "paint": {"line-color": "rgba(21, 101, 192, 1)"}
    },
    {
      "id": "roadl",
      "type": "line",
      "source": "hands-on",
      "source-layer": "roadl",
      "minzoom": 6,
      "maxzoom": 22,
      "paint": {"line-color": "rgba(194, 24, 91, 1)"}
    },
    {
      "id": "riverl",
      "type": "line",
      "source": "hands-on",
      "source-layer": "riverl",
      "minzoom": 6,
      "maxzoom": 22,
      "paint": {"line-color": "rgba(21, 101, 192, 1)"}
    },
    {
      "id": "inwatera",
      "type": "fill",
      "source": "hands-on",
      "source-layer": "intwatera",
      "minzoom": 6,
      "maxzoom": 22
    },
    {
      "id": "polbndl",
      "type": "line",
      "source": "hands-on",
      "source-layer": "polbndl",
      "maxzoom": 22
    },
    {
      "id": "raill",
      "type": "line",
      "source": "hands-on",
      "source-layer": "raill",
      "maxzoom": 22
    },
    {
      "id": "airp",
      "type": "circle",
      "source": "hands-on",
      "source-layer": "airp",
      "maxzoom": 22,
      "layout": {},
      "paint": {"circle-radius": 3}
    },
    {
      "id": "builtupp",
      "type": "circle",
      "source": "hands-on",
      "source-layer": "builtupp",
      "maxzoom": 22,
      "paint": {"circle-radius": 2}
    },
    {
      "id": "landuse-fill",
      "type": "fill",
      "source": "hands-on",
      "source-layer": "landuse",
      "minzoom": 11,
      "maxzoom": 22,
      "filter": ["all", ["==", "$type", "Polygon"]],
      "paint": {"fill-opacity": 1, "fill-color": "rgba(38, 166, 154, 1)"}
    },
    {
      "id": "landuse-line",
      "type": "line",
      "source": "hands-on",
      "source-layer": "landuse",
      "paint": {"line-color": "rgba(0, 137, 123, 1)"}
    },
    {
      "id": "natural-fill",
      "type": "fill",
      "source": "hands-on",
      "source-layer": "natural",
      "filter": ["all", ["==", "$type", "Polygon"]],
      "layout": {},
      "paint": {"fill-color": "rgba(38, 198, 218, 1)"}
    },
    {
      "id": "natural-line",
      "type": "line",
      "source": "hands-on",
      "source-layer": "natural",
      "filter": ["all", ["==", "$type", "LineString"]],
      "layout": {},
      "paint": {"line-color": "rgba(0, 172, 193, 1)"}
    },
    {
      "id": "boundary",
      "type": "fill",
      "source": "hands-on",
      "source-layer": "boundary",
      "minzoom": 11
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "hands-on",
      "source-layer": "waterway",
      "paint": {"line-color": "rgba(21, 101, 192, 1)"}
    },
    {
      "id": "highway",
      "type": "line",
      "source": "hands-on",
      "source-layer": "highway",
      "paint": {"line-color": "rgba(194, 24, 91, 1)"}
    },
    {
      "id": "building-line",
      "type": "line",
      "source": "hands-on",
      "source-layer": "building",
      "filter": ["all"]
    },
    {
      "id": "building-fill",
      "type": "fill",
      "source": "hands-on",
      "source-layer": "building",
      "filter": ["all", ["==", "$type", "Polygon"]],
      "layout": {},
      "paint": {"fill-color": "rgba(189, 189, 189, 1)"}
    },
    {
      "id": "railway",
      "type": "line",
      "source": "hands-on",
      "source-layer": "railway"
    },
    {
      "id": "route",
      "type": "line",
      "source": "hands-on",
      "source-layer": "route",
      "paint": {"line-color": "rgba(230, 81, 0, 1)", "line-width": 3}
    },
    {
      "id": "aeroway",
      "type": "line",
      "source": "hands-on",
      "source-layer": "aeroway",
      "paint": {"line-width": 5}
    },
    {
      "id": "place",
      "type": "symbol",
      "source": "hands-on",
      "source-layer": "place",
      "layout": {"text-field": "{name}", "text-font": ["sans", "sans-si", "sans-ta"]},
      "paint": {"text-color": "rgba(0, 0, 0, 1)"}
    }
  ],
  "id": "w771tz9g4"
}
